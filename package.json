{
  "name": "redux-save",
  "version": "0.0.1",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist esm",
    "test": "jest",
    "test:cov": "jest --coverage",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src/*.ts --ignore src/types.ts --extensions .ts --out-dir lib ",
    "build:esm": "babel src/*.ts --ignore src/types.ts --extensions .ts --out-dir esm",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/redux-save.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup --bundleConfigAsCjs -c -o dist/redux-save.min.js",
    "build:types": "tsc",
    "build": "rimraf dist lib es && npm run build:types && npm run build:commonjs && npm run build:esm && npm run build:umd && npm run build:umd:min"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lodash": "^4.17.21",
    "redux": "^4.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.6.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
